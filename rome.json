{
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": false,
			"suspicious": {
				"noExplicitAny": "warn",
				"noAssignInExpressions": "off",
				"noDoubleEquals": "off"
			}
			
		}
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": true
		
	},
	"javascript": {
		"formatter": {
			"quoteProperties": "asNeeded",
			"quoteStyle": "single"
		}
	}
}
